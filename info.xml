<?xml version="1.0"?>
<extension key="contacts-summary-print" type="module">
  <file>contacts_summary_print</file>
  <name>Contacts Summary Print</name>
  <description>Summarize all contact details</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>OSSeed</author>
    <email>hello@osseed.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/vinay-osseed/contacts-summary-print</url>
    <url desc="Documentation">https://github.com/vinay-osseed/contacts-summary-print/blob/master/instructions.pdf</url>
    <url desc="Support">https://github.com/vinay-osseed/contacts-summary-print/blob/master/README.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-06-12</releaseDate>
  <version>1.1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.71</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ContactsSummaryPrint</namespace>
    <format>23.02.1</format>
    <angularModule>crmContactsSummaryPrint</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
